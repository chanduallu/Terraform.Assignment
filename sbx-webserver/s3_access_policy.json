{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ReadandWriteAccess",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObjectVersion",
                "s3:ListBucket",
                "s3:DeleteObject",
                "s3:GetObjectVersion"
            ],
            "Resource": [
                "arn:aws:s3:::${app}",
                "arn:aws:s3:::${app}/*"
            ]
        }
        
        
    ]
}